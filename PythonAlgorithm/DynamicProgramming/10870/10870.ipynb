    "def recursive(num) :\n",
    "    if num == 0:\n",
    "        return 0\n",
    "    elif num == 1:\n",
    "        return 1\n",
    "    return recursive(num-1) + recursive(num -2)\n",
    "input_num = int(input())\n",
    "print(recursive(input_num))"
   ]
